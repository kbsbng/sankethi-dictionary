[
    {
        "settings": [ "master" ],
        "appPort": "80",
        "middleware": [ "middleware/bodyParser.js",
            "middleware/cookieParser.js",
            "middleware/session.js",
            "middleware/auth.js",
            "middleware/verifyAuth.js" ],
        "specs": {
            "login": {
                "type": "HTMLFrameMojit",
                "config": {
                    "deploy": true,
                    "title": "Sankethi Dictionary - Login Page",
                    "child": {
                        "type": "LoginFrame",
                        "config": {
                            "children": {
                                "header": {
                                    "type": "Header"
                                },
                                "login": {
                                    "type": "Login"
                                },
                                "footer": {
                                    "type": "Footer"
                                }
                            }
                        }
                    }
                }
            },
            "searchPage": {
                "type": "HTMLFrameMojit",
                "config": {
                    "deploy": true,
                    "title": "Sankethi Dictionary - Search Page",
                    "child": {
                        "type": "SearchPage",
                        "config": {
                            "children": {
                                "header": {
                                    "type": "Header"
                                },
                                "search": {
                                    "type": "Search"
                                },
                                "footer": {
                                    "type": "Footer"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    {
        "settings": [ "environment:development" ],
        "staticHandling": {
            "forceUpdate": true
        }
    }
]
